import pymongo
import pandas as pd

# Connect to MongoDB Atlas (replace <your_connection_string>)
client = pymongo.MongoClient("<your_connection_string>")
db = client["mega_data_project"]
collection = db["airtravel_data"]

# Load CSV directly from URL
url = "https://people.sc.fsu.edu/~jburkardt/data/csv/airtravel.csv"
csv_data = pd.read_csv(url)

# Insert into MongoDB
collection.insert_many(csv_data.to_dict("records"))

# Query example: find months with travel > 400
results = list(collection.find({"JAN": {"$gt": 400}}, {"_id": 0, "Month": 1, "JAN": 1}))
print("Months with JAN passengers > 400:", results)
